        <Label Height="28" Margin="9,41,0,0" Name="label2" VerticalAlignment="Top" HorizontalAlignment="Left" Width="74">Wyrażenie B</Label>
        <Label Height="28" Margin="28,72,0,0" Name="label3" VerticalAlignment="Top" HorizontalAlignment="Left" Width="74">Operacja</Label>
        <TextBox Height="23" Margin="100,17,58,0" Name="tbA" VerticalAlignment="Top" />
        <TextBox Height="23" Margin="100,46,58,0" Name="tbB" VerticalAlignment="Top" />
        <RadioButton Height="16" Margin="100,0,0,27" Name="rbSr" IsChecked="True" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="31">;</RadioButton>
        <RadioButton Margin="100,0,0,8" Name="rbPrz" Height="16" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="31">,</RadioButton>
        <Button Height="23" HorizontalAlignment="Right" Margin="0,0,11,8" Name="btnAdd" VerticalAlignment="Bottom" Width="75" Click="btnAdd_Click">Dodaj</Button>
