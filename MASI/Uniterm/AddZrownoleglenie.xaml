<Window x:Class="Uniterm.AddZrownoleglenie"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uniterm"
        mc:Ignorable="d"
        Title="AddZrownoleglenie" 
        Height="158" Width="300" 
        ResizeMode="CanMinimize" 
        WindowStartupLocation="CenterScreen" 
        ShowInTaskbar="False">
    <Grid>
        <Label Height="28" Margin="9,15,0,0" Name="label1" VerticalAlignment="Top" HorizontalAlignment="Left" Width="74">Wyrażenie A</Label>
        <Label Height="28" Margin="9,41,0,0" Name="label2" VerticalAlignment="Top" HorizontalAlignment="Left" Width="74">Wyrażenie B</Label>
        <TextBox Height="23" Margin="100,17,58,0" Name="tbA" VerticalAlignment="Top" />
        <TextBox Height="23" Margin="100,46,58,0" Name="tbB" VerticalAlignment="Top" />
        <Button Height="23" HorizontalAlignment="Right" Margin="0,0,11,8" Name="btnAdd" VerticalAlignment="Bottom" Width="75" Click="btnAdd_Click">Dodaj</Button>
        <Label Height="28" Margin="9,41,0,0" x:Name="label2_Copy" VerticalAlignment="Top" HorizontalAlignment="Left" Width="74" Content="Wyrażenie B"/>
    </Grid>
</Window>
